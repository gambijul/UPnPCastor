<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>upnpcastor.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="upnpcastor.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="upnpcastor.ico" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\UPnPCastor.Core\UPnPCastor.Core.csproj" />
  </ItemGroup>

</Project>